      // <!--====== Main js ======-->
  <script>let option_value='<%= option_value %>'</script>
  <script type="text/javascript">
  
    if(option_value == 1){
      document.querySelector(".payment_service").classList.add("hide");
    }
    else if(option_value == 2){
      document.getElementById("selectedService").remove(0);
    }
    else if(option_value == 3){
      document.getElementById("selectedService").remove(1);
    }
    </script>
    

    
