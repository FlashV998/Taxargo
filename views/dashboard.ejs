                                <br>
                                <div class="card ">
                                <div class="card-body">
                                  
                                <h6 class="heading-content">Personal Data</h6>
                                    <table class="table table-striped">
                                        
                                        <tbody>
                                          <tr>
                                            <th scope="row "></th>
                                            <td>Username:</td>
                                            <td><%=data.userPersonalData[0].fname%> <%=data.userPersonalData[0].lname%></td>
                                            
                                          </tr>
                                          <tr>
                                            <th scope="row"></th>
                                            <td>Address:</td>
                                            <td><%=data.userPersonalData[0].flatno%>,<%=data.userPersonalData[0].streetname%>,<%=data.userPersonalData[0].localityname%>,<%=data.userPersonalData[0].townname%></td>
                                            <!-- <td>@fat</td> -->
                                          </tr>
                                          <tr>
                                            <th scope="row"></th>
                                            <td>Mobile No:</td>
                                            <td><%=data.userPersonalData[0].mobileno%></td>
                                            <!-- <td>@twitter</td>   -->
                                          </tr>
                                          <tr>
                                            <th scope="row"></th>
                                            <td>City:</td>
                                            <td><%=data.userPersonalData[0].townname%></td>
                                            <!-- <td>@twitter</td>   -->
                                          </tr>
                                          <tr>
                                            <th scope="row"></th>
                                            <td>State:</td>
                                            <td><%=data.userPersonalData[0].statename%></td>
                                            
                                          </tr>
                                          <tr>
                                            <th scope="row"></th>
                                            <td>Pincode:</td>
                                            <td><%=data.userPersonalData[0].pincode%></td>
                                            <!-- <td>@twitter</td>   -->
                                          </tr>
                                        </tbody>
                                      </table>  
                                 <br>
                                 <h6 class="heading-content">Uploaded Documents</h6>
                                 <div class="gap  row justify-content-around" style="width: auto;">
                                   <div class="col-md-4 " ><img style="width: 200px ;" src="data: <%= data.userPersonalData[0].savefile1[0].file1Type%>;charset=utf-8;base64,<%= arr[0]%>">
                                   </div>
                                   <div class="col-md-4"><img style="width: 200px ;" src="data: <%= data.userPersonalData[0].savefile1[1].file1Type%>;charset=utf-8;base64,<%= arr[1]%>">
                                   </div>
                                 </div>

                                 <div class="gap row justify-content-around">
