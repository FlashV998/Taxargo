                                  <div class="col-md-4"><img style="width: 200px;" src="data: <%= data.userPersonalData[0].savefile1[3].file1Type%>;charset=utf-8;base64,<%= arr[3]%>">
                                  </div>
                                </div>
                                    <div class="payment_service">
                                <div class="gap row justify-content-between ">
                                  <div class="card text-white bg-success mb-3 col-md-6" style="max-width: 20rem;">
                                      <div class="card-header text-center">ITR</div>
                                      <div class="card-body">
                                      <h4 id="priceElement1" class="card-title"><%= items.products[0].price %></h4>
                                      <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                                      </div>
                                  </div>
                                  <div class="card text-white bg-success mb-3 col-md-6" style="max-width: 20rem;">
                                      <div class="card-header text-center">GST</div>
                                      <div class="card-body">
                                      <h4 id="priceElement2" class="card-title"><%= items.products[1].price %></h4>
                                      <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                                  </div>
                                  </div>
                           </div>
                                
                                
                    

                                <div class="gap row justify-content-between ">
